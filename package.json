{
  "name": "pairprogramming",
  "displayName": "PairProgramming",
  "description": "Pair Programming",
  "version": "0.0.1",
  "publisher": "Abhinav",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "extension.chat",
        "title": "chat window"
      },
      {
        "command": "extension.connect",
        "title": "connect"
      },
      {
        "command": "extension.startPairProgramming",
        "title": "Start PairProgramming"
      },
      {
        "command": "extension.joinPairProgramming",
        "title": "Join PairProgramming"
      },
      {
        "command": "extension.sendFile",
        "title": "Send File"
      }
    ],
     "keybindings": [
       {
      "command": "extension.sayHello",
        "key": "ctrl+f1"
    },
    {
        "command": "extension.startPairProgramming",
        "key": "ctrl+f2"
    },
    {
        "command": "extension.joinPairProgramming",
        "key": "ctrl+f3"
    },
    {
        "command": "extension.chat",
        "key": "ctrl+f4"
    },{
        "command": "extension.sendFile",
        "key": "ctrl+f5"
    }]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "ip": "^1.1.3",
    "jquery": "^3.1.0",
    "net": "^1.0.2",
    "xmlhttprequest": "^1.8.0"
  }
}
